<?xml version="1.0" encoding="UTF-8" ?>
<CiscoIPPhoneDirectory>
{% for result in results -%}
 <DirectoryEntry>
  <Name>{{ result.name }}</Name>
  <Telephone>{{ result.number }}</Telephone>
 </DirectoryEntry>
{% else -%}
 <DirectoryEntry>
  <Name>No entries</Name>
  <Telephone></Telephone>
 </DirectoryEntry>
{% endfor -%}
</CiscoIPPhoneDirectory>
