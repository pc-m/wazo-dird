        "/personal": {
            "get": {
                "summary": "List personal contacts",
                "operationId": "list_personal",
                "description": "CSV format is the same as `/import`, where headers of all contacts are mixed. The charset of the response is always `utf-8`. Errors are always formatted in JSON.",
                "tags": [
                    "personal"
                ],
                "produces": [
                    "application/json",
                    "text/csv; charset=utf-8"
                ],
                "parameters": [
                    {
                        "name": "format",
                        "in": "query",
                        "type": "string",
                        "enum": ["application/json", "text/csv"],
                        "default": "application/json",
                        "required": false,
                        "description": "Format of the response body"
                    },
                    {
                        "name": "X-Auth-Token",
                        "in": "header",
                        "type": "string",
                        "required": true,
                        "description": "Authentication token obtained from xivo-auth."
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A list of personal contacts",
                        "schema": {
                            "properties": {
                                "items": {
                                    "$ref": "#/definitions/PersonalList"
                                }
                            }
                        }
                    },
                    "204": {
                        "description": "No contacts are available (CSV format only)."
                    },
                    "503": {
                        "description": "Another service is unavailable (e.g. xivo-auth, consul, ...)",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            },
            "post": {
                "summary": "Create new personal contact",
                "operationId": "add_personal",
                "tags": [
                    "personal"
                ],
                "parameters": [
                    {
                        "name": "contact",
                        "in": "body",
                        "description": "The attributes of the contact",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/PersonalBody"
                        }
                    },
                    {
                        "name": "X-Auth-Token",
                        "in": "header",
                        "type": "string",
                        "required": true,
                        "description": "Authentication token obtained from xivo-auth."
                    }
                ],
                "responses": {
                    "201": {
                        "description": "The new personal contact",
                        "schema": {
                            "$ref": "#/definitions/Personal"
                        }
                    },
                    "503": {
                        "description": "Another service is unavailable (e.g. xivo-auth, consul, ...)",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            },
            "delete": {
                "summary": "Purge personal contacts",
                "operationId": "purge_personal",
                "tags": [
                    "personal"
                ],
                "parameters": [
                    {
                        "name": "X-Auth-Token",
                        "in": "header",
                        "type": "string",
                        "required": true,
                        "description": "Authentication token obtained from xivo-auth."
                    }
                ],
                "responses": {
                    "204": {
                        "description": "The personal contacts was purged."
                    },
                    "503": {
                        "description": "Another service is unavailable (e.g. xivo-auth, consul, ...)",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                }
            }
        },
