{
    "swagger": "2.0",
    "info": {
        "title": "XiVO Dird",
        "description": " Contacts and directories in XiVO are managed by the xivo-dird daemon. This service provides a public API that can be used to query the directories that are configured on a XiVO. \n\n\nA contact can contain various information such as a person's phone number, number, office number, department, office number, etc. The information displayed is selected via a profile. Directories can be aggregated using multiple data sources such as an LDAP server, a CSV file, another XiVO server, etc.\n\n\nPlease refer to [the documentation](http://documentation.xivo.io) for further details.\n\n\nNote: The 0.1 API is currently in development. Major changes could still happen and new resources will be added over time.",
        "version": "0.1",
        "license": {
            "name": "GPL v3",
            "url": "http://www.gnu.org/licenses/gpl.txt"
        },
        "contact": {
            "name": "Dev Team",
            "url": "http://xivo.io",
            "email": "dev@avencall.com"
        }
    },
    "x-xivo-port": 9489,
    "x-xivo-name": "dird",
    "schemes": [
        "https"
    ],
    "basePath": "/0.1",
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/directories/menu/{profile}/cisco": {
            "get": {
                "summary": "Extends the directories menu for Cisco Unified IP Phones",
                "description": "The `menu` query will return a XML to extend the directories menu for Cisco Unified IP Phones.",
                "operationId": "cisco_menu",
                "tags": [
                    "directories",
                    "cisco",
                    "phone directories"
                ],
                "produces": [
                    "text/xml"
                ],
                "responses": {
                    "200": {
                        "description": "XML to extend the directories menu",
                        "schema": {
                            "$ref": "#/definitions/CiscoIPPhoneMenu"
                        },
                        "examples": {
                            "text/xml": "<CiscoIPPhoneMenu> <MenuItem> <Name>XiVO Directory</Name> <URL>http://proxy-url?vendor=cisco'&amp;xivo_user_uuid=1234</URL> </MenuItem> </CiscoIPPhoneMenu>"
                        }
                    },
                    "404": {
                        "description": "The lookup profile does not exist",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    },
                    "503": {
                        "description": "Another service is unavailable (e.g. xivo-auth, consul, ...)",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "profile",
                        "in": "path",
                        "type": "string",
                        "description": "The profile defines the list of directories to query and how the results are displayed. Used to generate lookup URL",
                        "required": true
                    },
                    {
                        "name": "X-Auth-Token",
                        "in": "header",
                        "type": "string",
                        "required": true,
                        "description": "Authentication token obtained from xivo-auth."
                    },
                    {
                        "name": "Proxy-URL",
                        "in": "header",
                        "type": "string",
                        "required": false,
                        "description": "URL that will be integrated in the XML response. (not working on swagger 2.1"
                    }
                ]
            }
        },
        "/directories/lookup/{profile}/cisco": {
            "get": {
                "summary": "Search for contacts with Cisco Unified IP Phones",
                "description": " The `lookup` query will return a list of result matching the searched term formated for Cisco Unified IP Phones. The result will be retrieved from all configured directories for the given profile.\n\n\nThis route is provided by the `cisco_view` plugin using the `lookup` plugin and all configured sources for the given profile.",
                "operationId": "cisco_lookup",
                "tags": [
                    "directories",
                    "cisco",
                    "phone directories"
                ],
                "produces": [
                    "text/xml"
                ],
                "responses": {
                    "200": {
                        "description": "XML to extend the directories menu",
                        "schema": {
                            "$ref": "#/definitions/CiscoIPPhoneDirectory"
                        },
                        "examples": {
                            "text/xml": " <CiscoIPPhoneDirectory> <DirectoryEntry> <Name>Alice</Name> <Telephone>5555555555</Telephone> </DirectoryEntry> </CiscoIPPhoneDirectory><SoftKeyItem> <Name>PrevPage</Name> <URL>https://xivo-dird:9489/0.1/directories/lookup/default_phone/cisco?xivo_user_uuid=000000-0000-0000-0000-000000000000&amp;term=Alice&amp;limit=1&amp;offset=1</URL> <Position>2</Position> </SoftKeyItem> <SoftKeyItem> <Name>Exit</Name> <URL>SoftKey:Exit</URL> <Position>3</Position> </SoftKeyItem> <SoftKeyItem> <Name>NextPage</Name> <URL>https://xivo-dird:9489/0.1/directories/lookup/default_phone/cisco?xivo_user_uuid=000000-0000-0000-0000-000000000000&amp;term=Alice&amp;limit=1&amp;offset=2</URL> <Position>4</Position> </SoftKeyItem> </CiscoIPPhoneDirectory>"
                        }
                    },
                    "404": {
                        "description": "The lookup profile does not exist",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    },
                    "503": {
                        "description": "Another service is unavailable (e.g. xivo-auth, consul, ...)",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "profile",
                        "in": "path",
                        "type": "string",
                        "description": "The profile defines the list of directories to query and how the results are displayed. Used to generate lookup URL",
                        "required": true
                    },
                    {
                        "name": "term",
                        "in": "query",
                        "type": "string",
                        "description": "A search term to look for",
                        "required": false
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "type": "integer",
                        "description": "The limit defines the number of individual objects that are returned. Default: 16",
                        "default": 0,
                        "required": false
                    },
                    {
                        "name": "offset",
                        "in": "query",
                        "type": "integer",
                        "description": "The offset defines the offsets the start by the number specified. Default: 0",
                        "default": 0,
                        "required": false
                    },
                    {
                        "name": "X-Auth-Token",
                        "in": "header",
                        "type": "string",
                        "required": true,
                        "description": "Authentication token obtained from xivo-auth."
                    },
                    {
                        "name": "Proxy-URL",
                        "in": "header",
                        "type": "string",
                        "required": false,
                        "description": "URL that will be integrated in the XML response. (not working on swagger 2.1"
                    }
                ]
            }
        },
